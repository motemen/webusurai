{
  "name": "webusurai",
  "version": "1.0.0",
  "main": "dist/worker.js",
  "scripts": {
    "build": "webpack",
    "format": "prettier --write  '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "lint": "eslint --max-warnings=0 src && prettier --check '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "test": "concurrently 'yarn:test:*'",
    "test:workers": "jest",
    "test:frontend": "( cd frontend && yarn test --watchAll=false )",
    "start": "concurrently --names 'workers,frontend' 'miniflare' 'cd frontend && yarn start'",
    "deploy": "( cd frontend && yarn build ) && wrangler publish"
  },
  "author": "motemen <motemen@gmail.com>",
  "license": "MIT",
  "eslintConfig": {
    "root": true,
    "extends": [
      "typescript",
      "prettier"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@cloudflare/workers-types": "^3.3.1",
    "@types/jest": "^26.0.23",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-jest": "^27.4.6",
    "concurrently": "^7.0.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-typescript": "^3.0.0",
    "jest": "^27.0.1",
    "jest-environment-miniflare": "^2.2.0",
    "miniflare": "^2.2.0",
    "prettier": "^2.3.0",
    "service-worker-mock": "^2.0.5",
    "ts-jest": "^27.0.1",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.2.2",
    "random": "^3.0.6",
    "random-seedable": "^1.0.8"
  }
}
